Length_of_rhombus = int(input("Enter the length of Rhombus: "))
for i in range(1, Length_of_rhombus + 1):
    if i == 1 or i == Length_of_rhombus:
        print(" " * (Length_of_rhombus - i) + "*" * Length_of_rhombus)
    else:
        print(" " * (Length_of_rhombus - i) + "*" + " " * (Length_of_rhombus - 2) + "*")